(define on-line-read
        Str -> (trap-error
                  (let Sexp (read-from-string Str)
                       KL (shen.elim-def (shen.proc-input+ (hd Sexp)))
                       (trap-error
                          [ok (eval-kl KL)]
                          (/. X [err (error-to-string X)])))
                  (/. X [more])))