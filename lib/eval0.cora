(set 'set (set))
(set 'car (car))
(set 'cdr (cdr))
(set 'cons (cons))
(set 'cons (cons))
(set '+ (+))
(set '- (-))
(set '* (*))
(set '/ (/))
(set '= (=))
(set '> (>))
(set '< (<))
(set 'gensym (gensym))
(set 'symbol? (symbol?))
(set 'not (not))
(set 'string? (string?))

(defun eval0 (exp)
  (cond 
    ((symbol? exp) (value exp))
    ((or (number? exp) (string? exp) (boolean? exp) (null? exp))
     exp)
    ((cons? exp)
     (if (= (car exp) 'quote)
	 (cadr exp)
	 (apply (eval0 (car exp)) (map eval0 (cdr exp)))))))
